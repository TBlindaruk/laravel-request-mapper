{
  "name": "maksi/laravel-request-mapper",
  "authors": [
    {
      "name": "Tetiana Blindaruk",
      "email": "t.blindaruk@gmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "DTO",
    "request-mapper",
    "mapper"
  ],
  "license": "MIT",
  "require": {
    "doctrine/annotations": "^1.6",
    "doctrine/cache": "^2.0",
    "illuminate/contracts": "^8.0",
    "illuminate/http": "^8.0",
    "illuminate/support": "^8.0",
    "illuminate/validation": "^8.0",
    "symfony/validator": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Maksi\\LaravelRequestMapper\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maksi\\LaravelRequestMapper\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Maksi\\LaravelRequestMapper\\RequestMapperProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
